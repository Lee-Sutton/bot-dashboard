{
  "name": "bot-dashboard",
  "private": true,
  "scripts": {
    "cypress": "node_modules/.bin/cypress run",
    "cypress:open": "node_modules/.bin/cypress open",
    "start": "meteor run",
    "test": "BABEL_ENV=testing mocha ./tests --compilers js:babel-register",
    "mtest": "meteor --once test --driver-package meteortesting:mocha --port 4000",
    "mtest-watch": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --port 4000"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "bcrypt": "^2.0.1",
    "meteor-node-stubs": "^0.3.2",
    "snoostorm": "0.0.5",
    "snoowrap": "^1.15.2"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-meteor": "^7.0.0-beta.51",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cypress": "^3.0.1",
    "mocha": "^5.2.0",
    "testdouble": "^3.8.1"
  }
}
